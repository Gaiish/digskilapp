<!doctype html>
<html>
  <head>
    <title>Digital Skills for Africa</title>
    <link href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' rel='stylesheet'>

  </head>
  <body>
    <div class="container">
      <p style='padding:20px;'></p>
      <form action='/' method='POST'>
        <div class="form-group">
          <label for='name'>Name:</label>
          <input class="form-control" type='text' id='name' name='name' placeholder="Enter full name" />
        </div>
        <p></p>
        <div class="form-group">
          <label for='email'>Email:</label>
          <input class="form-control" type='text' id='email' type='email' name='email' placeholder="Enter email" />
        </div>
        <p></p>
        <div class="form-group">
          <label for='pnumber'>Phone number:</label>
          <input class="form-control" type='text' id='pnumber' name='pnumber' placeholder="Enter phone number" />
        </div>
        <p></p>
        <div class="form-group">
          <label for='gender'>Gender:</label>
          <select name='gender' class="form-control">
            <option value='M'>M</option>
            <option value='F'>F</option>
          </select>
        </div>
          <p></p>
          <button class="btn btn-primary" type='submit'>Send</button>
        </div>
      </form>
    </div>
    <script src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js'></script>
  </body>
</html>
